{
	"name": "web",
	"version": "0.1.0",
	"homepage": "/react/",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"aos": "^2.3.4",
		"autoprefixer": "^10.4.13",
		"concurrently": "^7.6.0",
		"postcss": "^8.4.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-multi-carousel": "^2.8.2",
		"react-router-dom": "^6.8.0",
		"react-scripts": "5.0.1",
		"react-scroll-parallax": "^3.3.2",
		"tailwind-styled-components": "^2.2.0",
		"tailwindcss": "^3.2.4",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"build:preview": "concurrently \"npx tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/tailwind.output.css\" \"yarn build\"",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"watch-tw": "npx tailwindcss -i ./src/assets/tailwind/tailwind.css -o ./src/assets/tailwind/tailwind.output.css --watch",
		"dev": "concurrently \"react-scripts start\" \"npx tailwindcss -i ./src/assets/tailwind/tailwind.css -o ./src/assets/tailwind/tailwind.output.css --watch\""
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"packageManager": "yarn@3.4.1",
	"devDependencies": {
		"prettier": "^2.8.3",
		"prettier-plugin-tailwindcss": "^0.2.2"
	}
}
